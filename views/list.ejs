<%- include("header") -%>

<%- include("nav") -%>

<table>
    <thead>
        <th>Name</th>
        <th>Problem</th>
        <th>Age</th>
    </thead>

    <% records.forEach(function(entry){ %>

        <% if(tdate==entry.date){ %>
        <tr>
            <td><%= entry.fname %></td>
            <td><%= entry.problem %></td>
            <td><%= entry.age %></td>
        </tr>
        <% } %>

    <% }); %>

</table>


<%- include("footer") -%>